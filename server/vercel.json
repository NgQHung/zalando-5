{
  "version": 2,
  "builds": [
    {
      "src": "./server.ts",
      "use": "@vercel/node",
      "config": { "serverFiles": ["package.json"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.ts",
      "continue": true,
      "headers": { "Cache-Control": "public, max-age=0, must-revalidate", "Service-Worker-Allowed": "/" }
    }
  ]
}
